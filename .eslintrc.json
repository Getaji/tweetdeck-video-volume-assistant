{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2016
  },
  "rules": {
    "no-console": "off",
    "no-extend-native": "off",
    "class-methods-use-this": "off",
    "max-classes-per-file": "off",
    "no-use-before-define": "off",
    "node/no-unpublished-require": "off",
    "node/no-missing-import": "off",
    "node/no-unpublished-import": "off"
  },
  "env": {
    "browser": true,
    "webextensions": true
  }
}